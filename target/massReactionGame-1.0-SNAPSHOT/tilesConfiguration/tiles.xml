    <definition name="base.definition"
        template="/views/layout.jsp">
        <put-attribute name="body" value="/views/main.jsp"/>
        <put-attribute name="header" value="/views/header.jsp" />
        <put-attribute name="title" value="" />


    </definition>



    <definition name="mainBody" extends="base.definition">
        <put-attribute name="title" value="MainBody" />
        <put-attribute name="body" value="/views/mainBody.jsp" />
    </definition>

    <definition name="registerCharacter" extends="base.definition">
        <put-attribute name="title" value="RegisterCharacter" />
        <put-attribute name="body" value="/views/registerCharacter.jsp" />
    </definition>

 
